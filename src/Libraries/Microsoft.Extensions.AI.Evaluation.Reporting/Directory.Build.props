  <Import Project="$([MSBuild]::GetPathOfFileAbove('Directory.Build.props', '$(MSBuildThisFileDirectory)../'))" />

  <PropertyGroup>
    <!-- Set the environment variable EnvEnableEvaluationReportBuild to enable the report build locally -->
