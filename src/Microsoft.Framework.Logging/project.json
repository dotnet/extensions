                "System.Linq": "4.0.0-beta-*",
                "System.Threading": "4.0.10-beta-*"
            }
        },
        ".NETPortable,Version=v4.6,Profile=Profile151": {
            "frameworkAssemblies": {
                "System.Collections": "",
                "System.Collections.Concurrent": "",
