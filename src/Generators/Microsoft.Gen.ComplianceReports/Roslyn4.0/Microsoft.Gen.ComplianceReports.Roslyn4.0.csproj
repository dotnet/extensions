<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>Microsoft.Gen.ComplianceReports</AssemblyName>
    <AnalyzerRoslynVersion>4.0</AnalyzerRoslynVersion>
    <RoslynApiVersion>$(MicrosoftCodeAnalysisVersion_4_0)</RoslynApiVersion>
    <DefineConstants>$(DefineConstants);ROSLYN_4_0_OR_GREATER</DefineConstants>
  </PropertyGroup>

  <PropertyGroup>
    <Stage>dev</Stage>
    <MinCodeCoverage>99</MinCodeCoverage>
    <MinMutationScore>85</MinMutationScore>
  </PropertyGroup>
</Project>
