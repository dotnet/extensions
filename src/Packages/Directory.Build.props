<Project>
  <PropertyGroup>
    <!-- Use TargetFrameworks since it can be overriden by .targetframeworks file -->
    <TargetFrameworks Condition="'$(TargetFrameworks)' == ''">netstandard2.0</TargetFrameworks>
    <GenerateDocumentationFile>n/a</GenerateDocumentationFile>
    <SkipAnalyzers>true</SkipAnalyzers>
    <DisableNETStandardCompatErrors>true</DisableNETStandardCompatErrors>
    <Api>false</Api>
  </PropertyGroup>

  <Import Project="$([MSBuild]::GetPathOfFileAbove('Directory.Build.props', '$(MSBuildThisFileDirectory)../'))" />
</Project>
