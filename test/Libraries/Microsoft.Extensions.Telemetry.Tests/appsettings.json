{
  "Logging": {
    "UseFormattedMessage": true
  },
  "MeteringWithOverrides": {
    "MeterState": "Enabled",
    "MeterStateOverrides": {
      "testMeter2": "Disabled",
      "DotNet.Test": "Disabled",
      "DotNet.Test.Internal": "Enabled",
      "DotNet.Test.External": "Disabled"
    }
  },
  "MeteringWithOverridesWithEmptyOverride": {
    "MeterState": "Enabled",
    "MeterStateOverrides": {
      "": "Disabled"
    }
  },
  "ProbabilitySampler": {
    "Rules": [
      {
        "Category": "Program.MyLogger",
        "LogLevel": "Information",
        "EventId": 1,
        "Probability": 1.0
      },
      {
        "LogLevel": "Information",
        "Probability": 0.01
      },
      {
        "LogLevel": "Warning",
        "Probability": 0.1
      }
    ]
  }
}
